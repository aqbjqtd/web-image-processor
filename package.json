{
  "name": "modern-image-processor",
  "version": "1.0.0",
  "description": "现代化智能图像批量处理工具 - 基于Vue.js 3和Quasar Framework",
  "main": "index.js",
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "serve": "quasar serve",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
    "format": "prettier --write \"**/*.{js,vue,json,css,scss,md}\"",
    "test": "vitest run",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.vue --fix",
    "version:sync": "node scripts/sync-version.js",
    "version:patch": "node scripts/sync-version.js bump patch",
    "version:minor": "node scripts/sync-version.js bump minor",
    "version:major": "node scripts/sync-version.js bump major"
  },
  "keywords": [
    "image-processing",
    "batch-processing",
    "vue3",
    "quasar",
    "canvas"
  ],
  "author": "现代图像处理工具开发团队",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aqbjqtd/web-image-processor.git"
  },
  "bugs": {
    "url": "https://github.com/aqbjqtd/web-image-processor/issues"
  },
  "homepage": "https://github.com/aqbjqtd/web-image-processor#readme",
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "file-saver": "^2.0.5",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.7.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "happy-dom": "^18.0.1",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "prettier": "^3.2.4",
    "typescript": "^5.9.2",
    "vite": "^2.9.18",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.19.0",
    "yarn": ">= 1.21.1"
  }
}
